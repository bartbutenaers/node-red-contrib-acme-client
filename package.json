    },
    "homepage": "https://github.com/bartbutenaers/node-red-contrib-acme-client",
    "repository": {
        "type": "git",
        "url": "https://github.com/bartbutenaers/node-red-contrib-acme-client.git"
    },
    "node-red": {
        "nodes": {
